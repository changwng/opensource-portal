extends ../../layout

block content
  .container
    h1 Dropped

    if results
      h3 Results
      ul
        each result in results
          li= result
    else
      p No results. No operations were performed.

    if entity
      h3 Link information and/or GitHub user data
      pre= JSON.stringify(entity, undefined, 2)
